"VertexLitGeneric"
{
	"$baseTexture" "models/player/items/medic/hwn_medic_misc2"
	"$bumpmap" "models/effects/flat_normal"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"

	"$selfillum" "1"
	"$selfillumtint" "[.7 .10 .04]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}