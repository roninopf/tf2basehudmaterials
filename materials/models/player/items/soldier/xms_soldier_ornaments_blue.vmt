"VertexLitGeneric"
{
	"$basetexture" "models/player/items/soldier/xms_soldier_ornaments"
	"$bumpmap" "models/player/items/soldier/xms_soldier_ornaments_normals"
	
	"$detail" "effects/tiledfire/firelayeredslowtiled512"
	"$detailscale" 5.0
	"$detailblendfactor" 0.01
	"$detailblendmode" 6
	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" 1
	"$envmapcontrast" 0.25
	"$envmapsaturation" 1.0
	
	"$phong" 1
	"$phongexponent" 15.0
	"$phongboost" 4.0
	"$phongfresnelranges" "[0.4 0.8 2.0]"
	
	"$rimlight" 1
	"$rimlightexponent" 4.0
	"$rimlightboost" 2.0
	
	"$blendtintbybasealpha" 1
	
	"$yellow" 0
	"$colortint_base" "{17 60 80}"
	"$colortint_tmp" "[0 0 0]"
	"$envmaptintmulti" 4.0
	
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"Multiply"
  		{
   			"srcVar1" "$color2"
   			"srcVar2" "$envmaptintmulti"
   			"resultVar" "$envmaptint"
  		}
	}
}