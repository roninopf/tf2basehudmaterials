"VertexLitGeneric"
{
   "$baseTexture" "models/player/items/all_class/qc_flask_glass"
	"$additive" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.8 .8 .8]"

	"$phong" "1"
	"$phongexponent" "65"
	"$phongfresnelranges" "[1 2 4]"
//	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$phongboost" "6"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "2"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}