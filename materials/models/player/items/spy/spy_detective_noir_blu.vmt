"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/spy/spy_detective_noir_blu"
	"$bumpmap" "models/player/items/spy/spy_detective_noir_normal" 
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

 	"$blendtintcoloroverbase" "0"  

	"$colortint_base" "{69 79 88}"
	"$colortint_tmp" "[0 0 0]" 

	"$phong" "1"
	"$phongexponent" "8"
	"$phongboost" ".16"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.25 .5 .5]"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "2"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "0.5"										// Boost for ambient cube component of rim lighting

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}