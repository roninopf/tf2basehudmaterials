"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/pyro/mtp_backpack_blue"
	"$bumpmap"	"models/player/items/pyro/mtp_backpack_normal"
	"$envmap" "effects/saxxy/saxxy_gold"
	"$envmaptint" "[.15 .15 .15]"

	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "45"
	"$phongboost" "2"	
	"$lightwarptexture" "models/lightwarps/rainblower_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "1"
	
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}